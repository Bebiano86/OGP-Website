<game_config>
  <game_key>minecraftpe_linux32</game_key>
  <protocol>gameq</protocol>
  <gameq_query_name>minecraftpe</gameq_query_name>
  <game_name>Minecraft Bedrock Edition</game_name>
  <server_exec_name>bedrock_server</server_exec_name>
  <query_port type="add">1</query_port>
  <cli_template>bedrock_server</cli_template>
  <console_log>logs/latest.log</console_log>
  <max_user_amount>250</max_user_amount>
  <mods>
    <mod key="default">
      <name>None</name>
    </mod>
  </mods>
  <replace_texts>
    <text key="ip">
      <default>server-ip=.*</default>
      <var>server-ip=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="port">
      <default>server-port=.*</default>
      <var>server-port=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="query_port">
      <default>server-portv6=.*</default>
      <var>server-portv6=</var>
      <filepath>server.properties</filepath>
    </text>
    <text key="max_players">
      <default>max-players=.*</default>
      <var>max-players=</var>
      <filepath>server.properties</filepath>
    </text>
  </replace_texts>
  <environment_variables>
    export LD_LIBRARY_PATH={OGP_HOME_DIR} {OGP_HOME_DIR}/bedrock_server
  </environment_variables>
</game_config>
